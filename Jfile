pipeline {
    agent any
    stages {
              
        stage ('build'){
            steps {
                
                    sh 'mvn clean install'
                
            }
        }
        
        stage  ('test'){
            steps {                
               post{ 
                    archiveArtifacts artifacts: 'backend/target/*.jar', followSymlinks: false
                    archiveArtifacts artifacts: 'frontend/target/*.jar', followSymlinks: false
                }
            }
        }
    }
}
